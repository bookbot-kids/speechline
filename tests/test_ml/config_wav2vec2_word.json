{
    "do_classify": false,
    "filter_empty_transcript": true,
    "transcriber": {
        "type": "wav2vec2",
        "model": "facebook/wav2vec2-base-960h",
        "return_timestamps": "word",
        "chunk_length_s": 30
    },
    "segmenter": {
        "type": "word_overlap",
        "minimum_chunk_duration": 0.7
    }
}