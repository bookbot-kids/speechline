{
    "do_classify": true,
    "classifier": {
        "model": "bookbot/distil-wav2vec2-adult-child-cls-52m",
        "max_duration_s": 3.0,
        "batch_size": 128
    },
    "transcriber": {
        "type": "wav2vec2",
        "model": "bookbot/wav2vec2-ljspeech-gruut",
        "return_timestamps": "char",
        "chunk_length_s": 30
    },
    "segmenter": {
        "silence_duration": 0.3,
        "minimum_chunk_duration": 0.7
    }
}