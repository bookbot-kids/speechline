{
    "do_classify": true,
    "classifier": {
        "model": "bookbot/distil-wav2vec2-adult-child-cls-52m",
        "max_duration_s": 3.0,
        "batch_size": 128
    },
    "transcriber": {
        "type": "whisper",
        "model": "openai/whisper-tiny",
        "return_timestamps": true,
        "chunk_length_s": 0
    },
    "segmenter": {
        "type": "silence",
        "silence_duration": 0.3,
        "minimum_chunk_duration": 0.7
    }
}